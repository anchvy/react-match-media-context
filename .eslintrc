{
  "plugins": ["prettier", "react-hooks"],
  "extends": ["react-app", "airbnb", "plugin:prettier/recommended", "eslint:recommended", "prettier/react"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": "off",
    "react/prop-types": "off"
  }
}